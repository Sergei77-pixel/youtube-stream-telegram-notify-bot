###############################################
# Используйте этот файл для ПРОДАКШЕНА
# Заполните значения ниже перед запуском
###############################################

# Токен вашего Telegram-бота (от @BotFather)
TELEGRAM_BOT_TOKEN=

# Один ключ YouTube API (если используете ровно один)
# YT_API_KEY=AIzaSyA_example_single_key_12345

# Или список ключей YouTube API (ротация при исчерпании квоты)
# Разделяйте запятой или точкой с запятой
# YT_API_KEYS=AIzaSyA_key1_111,AIzaSyA_key2_222,AIzaSyA_key3_333
# YT_API_KEYS=AIzaSyA_key1_111;AIzaSyA_key2_222;AIzaSyA_key3_333
YT_API_KEYS=

# Как часто проверять каналы (секунды)
POLL_INTERVAL=120

# Путь к файлу хранилища
STORAGE_PATH=data/storage.json

# (необязательно) Разрешённые пользователи для управления ботом (телеграм ID через запятую или ;) 
ALLOWED_USER_IDS=

# (необязательно) Кулдаун после обнаружения эфира (секунды)
COOLDOWN_SECONDS=3600
